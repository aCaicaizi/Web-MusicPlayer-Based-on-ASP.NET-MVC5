
@switch (ViewBag.type)
{
    case 1:
        {
            @model string
            ViewBag.Title = "搜索";
            Layout = "~/Views/Shared/Player_Layout.cshtml";


            Html.RenderAction("ArtistOrAlbum", new { s = Model });
            <div class="card">
                <div class="card-header">
                    <ul class="nav nav-tabs card-header-tabs">
                        <li class="nav-item">
                            @Ajax.ActionLink("单曲", "MusicSearch", "Music", new { s = Model, Type = 1 }, new AjaxOptions { OnComplete = "AriaActive(1)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link active a1", aria_current = "true" })
                        </li>
                        <li class="nav-item">
                            @Ajax.ActionLink("专辑", "AlbumSearch", "Music", new { s = Model, Type = 1 }, new AjaxOptions { OnComplete = "AriaActive(2)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a2" })

                        </li>
                        <li class="nav-item">
                            @Ajax.ActionLink("MV", "MVSearch", "Music", new { s = Model, Type = 1 }, new AjaxOptions { OnComplete = "AriaActive(3)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a3" })

                        </li>
                        <li class="nav-item">
                            @Ajax.ActionLink("歌单", "SheetSearch", "Music", new { s = Model, Type = 1 }, new AjaxOptions { OnComplete = "AriaActive(4)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a4" })

                        </li>
                        <li class="nav-item">
                            @Ajax.ActionLink("用户", "UserSearch", "User", new { s = Model, Type = 1 }, new AjaxOptions { OnComplete = "AriaActive(5)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a5 " })

                        </li>
                        <li class="nav-item">
                            @Ajax.ActionLink("歌词", "LyricsSearch", "Music", new { s = Model, Type = 1 }, new AjaxOptions { OnComplete = "AriaActive(6)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a6" })

                        </li>
                    </ul>
                </div>

                <div class="card-body" id="cardBody">
                    @Html.Action("MusicSearch", new { s = Model, Type = 1 })

                </div>
            </div>
            <script type="text/javascript">
                function AriaActive(e) {
                    $('.active').removeAttr('aria-current');
                    $('.active').removeClass('active');
                    $('.a' + e).toggleClass('active');
                    $('.a' + e).attr('aria-current', 'true');
                }
            </script>
            break;
        }
    case 2:
        {
            <div class="card">
                <div class="card-header">
                    <ul class="nav ">
                        <li class="nav-item">
                            @Ajax.ActionLink("单曲", "MusicSearch", "Music", new { s = Model, Type = 5 }, new AjaxOptions { OnComplete = "AriaActive(1)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a1" })
                        </li>
                        <li class="nav-item">
                            @Ajax.ActionLink("歌单", "SheetSearch", "Music", new { s = Model, Type = 2 }, new AjaxOptions { OnComplete = "AriaActive(4)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a4" })

                        </li>
                    </ul>
                </div>
            </div>
            <div class="card-body" id="cardBody">
                @Html.Action("MusicSearch", new { s = Model, Type = 5 })

            </div>
            <script type="text/javascript">
            </script>
            break;
        }
    case 3:
        {
            <div class="card">
                <div class="card-header">
                    <ul class="nav ">
                        <li class="nav-item">
                            @Ajax.ActionLink("歌曲", "MusicSearch", "Music", new { s = Model, Type = 5 }, new AjaxOptions { OnComplete = "AriaActive(1)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a1" })
                        </li>
                        <li class="nav-item">
                            @Ajax.ActionLink("专辑", "AlbumSearch", "Music", new { s = Model, Type = 1 }, new AjaxOptions { OnComplete = "AriaActive(2)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a2" })

                        </li>
                        <li class="nav-item">
                            @Ajax.ActionLink("MV", "MVSearch", "Music", new { s = Model, Type = 1 }, new AjaxOptions { OnComplete = "AriaActive(3)", UpdateTargetId = "cardBody", HttpMethod = "GET", InsertionMode = InsertionMode.Replace }, new { @class = "nav-link a3" })

                        </li>
                    </ul>
                </div>
            </div>
            <div class="card-body" id="cardBody">
                @Html.Action("MusicSearch", new { s = Model, Type = 5 })

            </div>
            <script type="text/javascript">
            </script>
            break;
        }
}
